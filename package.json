{
  "name": "node-docker",
  "version": "0.1.0",
  "private": true,
  "engines": {
    "node": "18.x"
  },
  "scripts": {
    "db:populate": "tsx dev/createDatabase.ts",
    "dev:server": "ts-node-dev backend/server.ts",
    "build:server": "tsc -p tsconfig.backend.json",
    "server:start": "node backend/dist/index.js"
  },
  "eslintConfig": {
    "extends": [
      "react-app",
      "plugin:storybook/recommended"
    ]
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  },
  "dependencies": {
    "express": "^4.19.2",
    "sqlite": "^5.1.1",
    "sqlite3": "^5.1.7",
    "typescript": "^4.9.5"
  },
  "devDependencies": {
    "@types/express": "^4.17.21",
    "@types/node": "^16.18.71",
    "@types/react-dom": "^18.2.18",
    "@types/react": "^18.2.48",
    "ts-node-dev": "^2.0.0",
    "tsx": "^4.7.0"
  }
}
